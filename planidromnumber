
public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		reversenumber(555);
	}
	
	public static void reversenumber(int num){
	    int temp=num;
	    int sum =0;
	    int r=0;
	    boolean flag=false;
	    while(num!=0){
	        r= num%10;
	        sum= (sum*10)+r;
	        num=num/10;
	         }
	    if(temp==sum){
	        flag=true;
	    }
	    
	    if(flag==true){
	        System.out.println("palindrom number");
	    }
	    else{
	       System.out.println(" not palindrom number"); 
	    }
	}
}
